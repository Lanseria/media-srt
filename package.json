{
  "name": "fast-vite-electron",
  "version": "0.0.1",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently -n=vite,main -c=#646cff,#74b1be \"npm run dev:vite\" \"npm run dev:main\"",
    "dev:vite": "vite",
    "dev:main": "rimraf dist/main && node -r ts-node/register script/build.main --env=development --watch",
    "build:vite": "vite build",
    "build:main": "rimraf dist/main && node -r ts-node/register script/build.main --env=production",
    "build": "npm run build:vite && npm run build:main && electron-builder --config electron-builder.config.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "bytenode": "^1.3.4",
    "pinia": "^2.0.11",
    "reflect-metadata": "^0.1.13",
    "store": "^2.0.12",
    "vue": "^3.2.4",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.18",
    "@types/minimist": "^1.2.2",
    "@types/wait-on": "^5.3.1",
    "@vitejs/plugin-vue": "^1.10.0",
    "@vue/compiler-sfc": "^3.2.22",
    "chalk": "^4.1.2",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "electron": "16.0.6",
    "electron-builder": "^22.14.5",
    "esbuild": "^0.13.15",
    "minimist": "^1.2.5",
    "naive-ui": "^2.24.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vue-tsc": "^0.29.6",
    "wait-on": "^6.0.0"
  }
}
