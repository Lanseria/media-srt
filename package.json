{
  "name": "media-srt",
  "version": "16.0.9",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently -n=vite,main -c=#646cff,#74b1be \"npm run dev:vite\" \"npm run dev:main\"",
    "dev:vite": "vite",
    "dev:main": "rimraf dist/main && node -r ts-node/register script/build.main --env=development --watch",
    "build:vite": "vite build",
    "build:main": "rimraf dist/main && node -r ts-node/register script/build.main --env=production",
    "build": "npm run build:vite && npm run build:main && electron-builder --config electron-builder.config.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^7.6.2",
    "@vueuse/rxjs": "^7.6.2",
    "bytenode": "^1.3.4",
    "dayjs": "^1.10.7",
    "dexie": "^3.2.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nanoid": "^3.3.1",
    "pinia": "^2.0.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "store": "^2.0.12",
    "tencentcloud-sdk-nodejs": "^4.0.291",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.18",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/minimist": "^1.2.2",
    "@types/mockjs": "^1.0.6",
    "@types/wait-on": "^5.3.1",
    "@types/which": "^2.0.1",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.31",
    "chalk": "^4.1.2",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.13",
    "esbuild": "^0.13.15",
    "minimist": "^1.2.5",
    "naive-ui": "^2.25.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vue-tsc": "^0.29.8",
    "wait-on": "^6.0.1"
  }
}
